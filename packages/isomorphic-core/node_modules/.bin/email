#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/react-email/dist/cli/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/react-email/dist/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/react-email/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/react-email/dist/cli/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/react-email/dist/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules/react-email/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/react-email@3.0.7_react-dom@19.0.0_react@19.0.0__react@19.0.0/node_modules:/Applications/MAMP/htdocs/commerapp/panel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-email/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../react-email/dist/cli/index.js" "$@"
fi
